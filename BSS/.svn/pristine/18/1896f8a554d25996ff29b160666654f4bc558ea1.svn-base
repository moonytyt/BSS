package kr.or.ddit.organization.service;

import java.util.List;

import javax.inject.Inject;

import org.springframework.stereotype.Controller;

import kr.or.ddit.department.vo.DepartmentVO;
import kr.or.ddit.organization.dao.OrganizationDAO;

@Controller
public class OrganizationServiceImpl implements OrganizationService {
	
	@Inject
	private OrganizationDAO orDAO;
	
	@Override
	public List<DepartmentVO> retrieveDepartmentsList() {
		return orDAO.departmentsList();
	}
	
	@Override
	public List<DepartmentVO> retrieveDepartmentsDetailList(String depCd) {
		return orDAO.departmentsDetailList(depCd);
	}

}
